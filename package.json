{
  "name": "@jameslnewell/scaffold",
  "version": "1.0.0",
  "description": "A tool for scaffolding your projects",
  "type": "module",
  "bin": "dist/cli",
  "main": "dist/lib",
  "dependencies": {
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.1",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.0.3",
    "@types/node": "^20.11.1",
    "chokidar": "^3.6.0",
    "jest": "^29.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
